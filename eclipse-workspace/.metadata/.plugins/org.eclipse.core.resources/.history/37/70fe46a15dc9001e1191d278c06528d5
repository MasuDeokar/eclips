package javaAllExamples;

public class StaticVar {
	
	String name;
	String address;
	String cityName;
	
	public StaticVar(String name,String address,String cityName)
	{
		this.name=name;
		this.address= address;
		this.cityName=cityName;		
	}
	public void getAddress() {
		System.out.println(address);
	}
	
	

	public static void main(String[] args) {
		
		StaticVar obj = new StaticVar("Shamanth","Lohegaon","Pune");
		StaticVar obj1 = new StaticVar("Vaishali","Dhamori","Pune");
		obj.getAddress();
		obj1.getAddress();

	}

}
