public class ParentClassDemo {
	String color = "red";
	
	public void gear()
	{
		System.out.println("Gear code is updated.");
	}
	public void breaks()
	{
		System.out.println("Breaks code is updated.");
	}
	
	public void audioSystem()
	{
		System.out.println("AudioSystem Parent code is updated.");
	}
	
	public static void main(String[] args) {
		
	
	}

}
