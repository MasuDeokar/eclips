package javaAllExamples;

public class PatternOnetoTen {

	public static void main(String[] args) {
		
		for(int i=0;i<=5;i++)
		{
			for(int j=0;j<=i;j++)
			{	
				System.out.println(" ");
			}
			for (int k=0;k<=i;k++)
			{
				int m=0;
				while(m<10)
				{
					System.out.println(m);
					k++;
				}
			}
		}

	}

}
