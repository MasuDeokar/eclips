package javaAllExamples;

import java.util.HashMap;

public class HashMapExample {

	public static void main(String[] args) {
		
		HashMap<Integer,String> hm = new HashMap<Integer,String>();
		hm.put(0, "Hello");
		hm.put(1, "Gudbye");
		hm.put(42, "Morning");
		hm.put(3, "Afternoon");
		hm.put(4, "Evening");
		hm.put(5, "GoodNight");
		System.out.println(hm.get(2));

	}

}
