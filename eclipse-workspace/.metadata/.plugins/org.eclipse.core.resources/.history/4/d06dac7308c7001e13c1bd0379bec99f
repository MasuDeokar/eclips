package javaAllExamples;

public class PatternThreeSixNine {

	public static void main(String[] args) {
		int k;
		for(int i=0;i<=5;i++)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.print("");
				
				}
			for( k=1;k<=i;k++)
			{
				System.out.print(k);
				System.out.print("\t");
				k++;
				
			}
			System.out.println();
		}

	}

}
