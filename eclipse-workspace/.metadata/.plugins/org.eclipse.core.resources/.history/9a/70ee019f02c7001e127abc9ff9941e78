package javaAllExamples;

public class PatternTentoOne {

	public static void main(String[] args) {
		
		int k=10;
		for (int i = 1; i <=5; i++) {
			for (int j = 1; j <=5 - i; j++) {
				System.out.print(k+"\t"); 
			//	System.out.println("\t"); // ("\t") use for space.
				k--;
			}	System.out.println();
			}
	}

}
