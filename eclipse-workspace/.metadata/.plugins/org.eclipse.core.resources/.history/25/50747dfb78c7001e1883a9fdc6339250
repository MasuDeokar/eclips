package javaAllExamples;

public class ArrayCisco {

	public static void main(String[] args) {
		
		int abc [][]= {{2,4,5},{3,2,10},{1,2,0}};
		int min =abc [0][0];
		int minCol = 0;
		
		for(int i=0;i<3;i++)
		{
			for(int j=0;j<3;j++)
			{
				if(abc[i][j]<min)
				{
					min=abc[i][j];
					minCol=j;
				}
			}
		}
		int max=abc[0][minCol];
		int i=0;
		while(i<3)
		{
			if(abc[i][minCol]>max);
			{
				max=abc[i][minCol];
			}
			i++;
		}
		System.out.println(max);
	}
	
}
